    "eslint-plugin-unused-imports": "^3.0.0",
    "globals": "^13.24.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-prettier": "^5.0.0",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "stylelint": "^16.0.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "stylelint": "^16.0.2",
    "typescript": "^5.3.3"
  },
  "name": "ultracite",
  "description": "Strict, opinionated ESLint config for modern TypeScript apps.",
  "version": "3.4.2",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haydenbleasel/ultracite.git"
  },
  "main": "./eslint.config.mjs",
  "keywords": [
    "ultracite",
    "eslint",
    "prettier",
    "stylelint",
    "typescript"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/eslint.config.mjs",
    "./prettier": "./dist/prettier.config.mjs",
    "./stylelint": "./dist/stylelint.config.mjs"
  },
  "author": "Hayden Bleasel <hello@haydenbleasel.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haydenbleasel/ultracite/issues"
  },
  "homepage": "https://github.com/haydenbleasel/ultracite#readme",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "conventional-commits",
      "first-time-contributor",
      "released"
    ]
  }
}
